doctype html
html
  head
    link(href='https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css', rel='stylesheet')
    script(src='https://ajax.googleapis.com/ajax/libs/angularjs/1.2.19/angular.js')
    script(type='text/javascript', src='https://www.google.com/jsapi')
    script(type='text/javascript', src='javascripts/chart-app.js')
    link(href='/stylesheets/style.css', type='text/css', rel='stylesheet')
  body(ng-app='simple-chart')
    div(ng-controller='MainController')
      nav.navbar.navbar-default.navbar-fixed-top
        .container
          .navbar-header
            button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='#navbar', aria-expanded='false', aria-controls='navbar')
              span.sr-only Toggle navigation
              span.icon-bar
              span.icon-bar
              span.icon-bar
            a.navbar-brand(href='#') Know Your SF Artists
          #navbar.navbar-collapse.collapse
            ul.nav.navbar-nav
              li.active
                a(href='#') Home
              li
                a(href='#about') About
              li
                a(href='#contact') Contact
              li.dropdown
                a.dropdown-toggle(href='#', data-toggle='dropdown', role='button', aria-haspopup='true', aria-expanded='false')
                  | Dropdown 
                  span.caret
                ul.dropdown-menu
                  li
                    a(href='#') Action
                  li
                    a(href='#') Another action
                  li
                    a(href='#') Something else here
                  li.divider(role='separator')
                  li.dropdown-header Nav header
                  li
                    a(href='#') Separated link
                  li
                    a(href='#') One more separated link
            ul.nav.navbar-nav.navbar-right
              li
                a(href='../navbar/') Default
              li
                a(href='../navbar-static-top/') Static top
              li.active
                a(href='./')
                  | Fixed top 
                  span.sr-only (current)
          // /.nav-collapse
      #chart_div.centered